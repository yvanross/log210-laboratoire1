{
    "name": "ets-log210-typescript-node-api",
    "version": "2.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "install-squelette": "git remote add squelette https://github.com/yvanross/log210-squelette.git || true && git checkout -b squelette squelette/main || true && git checkout squelette && git pull && git checkout main && git checkout squelette docs public src test views",
        "build": "tsc -b --verbose && shx --verbose cp -ru public dist/",
        "start": "node dist/index.js",
        "bs": "npm run build && open http://localhost:3000 && npm run start",
        "start:watch": "nodemon",
        "bsw": "npm run build && open http://localhost:3000  && npm run start:watch",
        "watch": "jest --watch --coverage",
        "test-sgbmock": "jest --colors --coverage \"test/sgbmock\"",
        "test-all": "jest --colors --coverage",
        "test-squelette": "jest --colors --coverage \"test/squelette\"",
        "uml-classes-puml": "tplant -i src/app.ts -A -o docs/modeles/App.puml",
        "uml-classes-svg": "tplant -i src/app.ts -A -o docs/modeles/App.svg",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm run build && npm start",
        "ext": "ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^4.3.1",
        "@types/debug": "^4.1.6",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^27.5.0",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.31",
        "@types/supertest": "^2.0.12",
        "chai": "^4.3.6",
        "chai-http": "^4.3.0",
        "coveralls": "^3.1.1",
        "gitinspector": "^0.5.0-dev-2",
        "http-request-mock": "^1.8.2",
        "jest": "^27.4.7",
        "jest-extended": "^1.1.0",
        "madge": "^5.0.1",
        "nodemon": "^2.0.16",
        "shx": "^0.3.4",
        "supertest": "^6.2.3",
        "tplant": "^3.1.0",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "axios": "^0.27.1",
        "debug": "^4.3.4",
        "express": "^4.18.1",
        "express-flash-plus": "^0.0.7",
        "express-session": "^1.17.2",
        "http-request": "^0.7.0",
        "jest-runner": "^28.1.0",
        "md5": "^2.3.0",
        "morgan": "^1.10.0",
        "pug": "^3.0.2",
        "runner": "^4.0.0"
    }
}
